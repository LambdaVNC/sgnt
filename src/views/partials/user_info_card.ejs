<% allParticipants.forEach(user => { %>
    <div class="col mb-4"> 
        <div class="card h-100 <%= user.role == "user" ? "border-left-info" : (user.role == "superAdmin" ? "border-left-danger" : "border-left-success") %>">
            <div class="card-body" style="color: black;">
                <h3 class="card-title <%= user.role == "user" ? "" : (user.role == "superAdmin" ? "text-danger" : "text-success") %>">
                    <%= user.firstName + " " + user.lastName %>
                </h3>
                <i class="fas fa-phone-alt text-success"></i> <%= user.phone %> <br>
                <i class="fas fa-envelope text-primary"></i> <%= user.email %> <br>
                <i class="fas fa-dollar-sign text-success"></i> Ã–dendi <br>
                <i class="fas fa-user"></i> <%= user.role %> <br>
            </div>
        </div>
    </div>
    <% }); %>